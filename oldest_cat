class Cat:
    species = 'mammal'
    def __init__(self, name, age):
        self.name = name
        self.age = age
    pass



# 1 Instantiate the Cat object with 3 cats

Tony = Cat('Tony',2)
Jane = Cat('Jane',6)
Jojo = Cat('Johnyy',5)

# 2 Find the oldest cat
def get_oldest_cat(*args):
  return max(*args)

# 3 Output

print(f'The oldest cat is {get_oldest_cat(Tony.age, Jane.age,Jojo.age)} years old.')
